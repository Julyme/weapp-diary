<view class='page'>
  <van-cell-group wx:for='{{verData}}' wx:key>
    <view class='{{index == 0 ? "current ver" : "ver" }}'>{{index == 0 ? "当前版本" : "历史版本"}} {{item.ver}}</view>
    <van-cell wx:for='{{item.text}}' wx:key wx:for-item='tItem' title="{{index+1}}.{{tItem}}"/>
  </van-cell-group>
</view>