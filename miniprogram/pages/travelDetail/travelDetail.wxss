}

.page {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  /* padding-top: 26rpx; */
}

.detail-bigImg {
  width: 100%;
  height: 100%;
}

.detail-bigImg image {
  width: 100%;
  height: 100%;
}

.detail-notice {
  background-color: #35c1f8;
  color: white;
}

.detail-header {
  width: 90%;
  margin: 12rpx auto;
  /* margin-bottom: 100rpx; */

  height: 92rpx;
  position: relative;
}

.detail-touxiang {
  height: 70rpx;
  width: 70rpx;
  margin-left: 8rpx;
  float: left;
  margin-top: 6rpx;
}

.detail-touxiang image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.detail-title {
  position: absolute;
  left: 102rpx;
  right: 10rpx;
  top: 0;
  height: 100%;
  display: flex;
  flex-flow: column nowrap;
}

.detail-title-top {
  color: #35c1f8;
  /* color: rgb(71, 203, 137); */
  font-size: 28rpx;
  font-weight: bold;
  width: 100%;
  height: 50%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  line-height: 50rpx;
  /* margin-top: 6rpx; */
}

.detail-title-bottom {
  width: 100%;
  height: 40%;
  font-size: 22rpx;
  /* color: #999; */
  color: rgb(114, 114, 114);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.detail-where {
  /* width: 100%; */
  position: absolute;
  font-size: 24rpx;
  color: #999;
  bottom: -30rpx;
  left: 118rpx;
  right: 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.detail-icon {
  margin-right: 2rpx;
}

.detail-container {
  width: 88%;
  height: auto;
  margin: 0 auto;
}

.detail-list {
  width: 100%;
  height: auto;
  /* background-color: white; */
  margin-bottom: 60rpx;
  position: relative;
}

.detail-time {
  width: 100%;
  height: 56rpx;
  line-height: 60rpx;
  color: #35c1f8;
  /* color: rgb(71, 203, 137); */
  font-size: 28rpx; 
  font-weight: bold;
  position: relative;
  box-sizing: border-box;
  padding-left: 6rpx;
  left: -26rpx;
}

.detail-times {
  display: inline-block;
  height: 100%;
  width: auto;
  box-sizing: border-box;
  padding-bottom: 4rpx;
  position: relative;
  bottom: 4rpx;
  left: 6rpx;
}

.detail-contact {
  position: absolute;
  width: 10rpx;
  top: 42rpx;
  bottom: 0;
  left: 12rpx;
  background-color: #35c1f8;
}

.detail-info {
  width: 96%;
  margin: 0 auto;
  margin-left: 3.5%;
  height: auto;
  /* background-color: white; */
  /* border-radius: 8rpx; */
  overflow: hidden;
  box-shadow: 0 2px 5px 1px rgba(0, 0, 0, 0.1);
  border-radius: 8rpx;
  border: 8rpx solid white;
  background-color: rgb(233, 233, 233);
}

.detail-list-img {
  text-align: center;
  display: flex;
  flex-flow: row wrap;
  /* justify-content: center; */
}

.detail-list-img image {
  vertical-align: top;
}

.detail-list-text {
  min-height: 100rpx;
  box-sizing: border-box;
  padding: 10rpx;
  position: relative;
  background-color: rgba(255,255,255,0.7);
}

.detail-list-top {
  width: 98%;
  min-height: 10rpx;
  height: auto;
  margin: 14rpx auto;
  /* background-color: gray; */
  font-size: 28rpx;
  color: rgb(114, 114, 114);
  margin-bottom: 30rpx;
}

.detail-list-bottom {
  position: absolute;
  /* width: 100rpx; */
  width: auto;
  height: 50rpx;
  left: 20rpx;
  bottom: 2rpx;
  font-size: 24rpx;
  color: #999;
  /* margin-bottom: 50rpx; */
}

.detail-list-bottom .detail-likenum {
  display: inline-block;
  height: 100%;
  width: auto;
  line-height: 50rpx;
  font-size: 30rpx;
  position: relative;
  left: 8rpx;
  top: -4rpx;
  color: #999;
}


.detail-img-one {
  width: 100%;
  height: 390rpx;
  margin: 0rpx auto 0 auto;
}

.detail-img-more {
  /* width: 32%; */
  width: 33.3333%;
  height: 200rpx;
  line-height: 200rpx;
  /* margin: 0rpx 1% 10rpx 1%; */
  box-sizing: border-box;
  position: relative;
}

.detail-img-more:first-child {
  margin-left: 0;
}

.detail-img-more:nth-child(3n+1) {
  margin-left: 0;
}

.detail-img-more:nth-child(3n + 3) {
  margin-right: 0;
}

.contact-border {
  position: absolute;
  width: 5rpx;
  background-color: #35c1f8;
  top:46rpx;
  bottom:-66rpx;
  left:-8rpx;
}

.detail-container .detail-list:last-child .contact-border {
  bottom:-16rpx;
}

.detail-container .detail-list:last-child .contact-border::after {
  /* content: '';
  position: absolute;
  background-color: #35c1f8;
  height: 6rpx;
  width: 36rpx;
  bottom: 0;
  left: -14rpx; */
}

.detail-container .contact-border-footer {
  position: absolute;
  bottom: -40rpx;
  left: -14rpx;
  color: #35c1f8;
  font-size: 24rpx;
}

.detail-heart {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  right: 20rpx;
  top: 18rpx;
  text-align: center;
  color: #35c1f8;
}


/* 评论 */
.chat {
  width: 100%;
  height: auto;
  padding-top: 50rpx;
  margin-bottom: 24rpx;
}

.chat-header {
  width: 94%;
  height: 60rpx;
  margin: 0 auto;
  font-size: 26rpx; 
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
  color: #35c1f8;
}

.chat-header-text {
  flex: 1;
  height: 100%;
  line-height: 54rpx;
  box-sizing: border-box;
  padding-left: 10rpx;
}

.chat-container {
  width: 100%;
  min-height: 60rpx;
  height: auto;
}

.chat-nowrap {
  width: 100%;
  height: 60rpx;
  /* box-sizing: border-box; */
  /* padding: 0 20rpx; */
  color: rgb(177, 177, 177);
  font-size: 26rpx;
  text-align: center;
  line-height: 60rpx;
  /* border-bottom: 10rpx solid rgb(223, 223, 223); */
  /* padding-bottom: 10rpx; */
  margin: 14rpx 0;
}

.chat-write {
  width: 100%;
  height: auto;
}

.chat-write-header {
  width: 94%;
  height: 60rpx;
  margin: 0 auto;
  font-size: 26rpx; 
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
  color: #35c1f8;
}

.chat-write-header-text {
  flex: 1;
  height: 100%;
  line-height: 58rpx;
  box-sizing: border-box;
  padding-left: 10rpx;
}

.chat-write-textarea {
  width: 96%;
  height: 280rpx;
  margin: 0 auto;
  box-sizing: border-box;
  border: 2rpx solid rgb(223, 223, 223);
  font-size: 26rpx;
}

.chat-write-textarea textarea {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 10rpx;
  padding-bottom: 70rpx;
  position: relative;
  letter-spacing: 4rpx;
  word-spacing: 4rpx;
}

.chat-write-footer {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 70rpx;
  border-top: 2rpx solid rgb(223, 223, 223);
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}

.chat-write-num {
  width: 40%;
  height: 100%;
  line-height: 70rpx;
  color: rgb(211, 211, 211);
  box-sizing: border-box;
  padding-left: 10rpx;
}

.chat-write-btn {
  width: 60%;
  height: 100%;
  box-sizing: border-box;
  padding-right: 10rpx;
  /* display: flex; */
}

.chat-write-btn button {
  width: 30%;
  height: 60rpx;
  margin: 0;
  margin-top: 5rpx;
  float: right;
}


/*评论主体*/
.chat-wrap-lists {
  width: 96%;
  height: auto;
  margin: 10rpx auto;
}

.chat-wrap-list {
  width: 100%;
  height: auto;
  margin: 10rpx 0;
  position: relative;
}

.chat-wrap-avatar {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  background-color: gray;
  overflow: hidden;
  position: absolute;
  left: 2rpx;

}

.chat-wrap-avatar image {
  width: 100%;
  height: 100%;
}

.chat-wrap-top {
  width: 100%;
  height: 50rpx;
  box-sizing: border-box;
  /* border: 1rpx solid gray; */
  padding-left: 76rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  font-weight: bold;
  color: rgb(155, 155, 155);
}

.chat-wrap-top text {
  font-size: 26rpx;
  color: #35c1f8;
  margin-right: 4rpx;
}

.chat-wrap-center {
  width: 100%;
  min-height: 100rpx;
  height: auto;
  box-sizing: border-box;
  /* border: 1rpx solid gray; */
  padding-left: 76rpx;
  font-size: 26rpx;
  line-height: 40rpx;
  color: rgb(155, 155, 155);
}

.chat-wrap-child {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  /* border: 1rpx solid gray; */
  padding-left: 76rpx;
  position: relative;
  margin-top: 4rpx;
}

.chat-wrap-avatar1 {
  left: 78rpx;
}